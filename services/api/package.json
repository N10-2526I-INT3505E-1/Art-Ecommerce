{
	"name": "api",
	"version": "1.0.50",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"db:generate:users": "drizzle-kit generate --config ./drizzle.config.users.ts",
		"db:push:users": "drizzle-kit push --config ./drizzle.config.users.ts",
		"db:generate:orders": "drizzle-kit generate --config ./drizzle.config.orders.ts",
		"db:push:orders": "drizzle-kit push --config ./drizzle.config.orders.ts",
		"db:generate:products": "drizzle-kit generate --config ./drizzle.config.products.ts",
		"db:push:products": "drizzle-kit push --config ./drizzle.config.products.ts",
		"db:generate:payments": "drizzle-kit generate --config ./drizzle.config.payments.ts",
		"db:push:payments": "drizzle-kit push --config ./drizzle.config.payments.ts",
		"db:generate:all": "bun run db:generate:users && bun run db:generate:orders && bun run db:generate:products && bun run db:generate:payments",
		"db:push:all": "bun run db:push:users && bun run db:push:orders && bun run db:push:products && bun run db:push:payments",
		"start": "bun run src/index.ts",
		"dev": "bun run --watch src/index.ts"
	},
	"dependencies": {
		"@elysiajs/cors": "^1.4.0",
		"@elysiajs/eden": "^1.4.5",
		"@elysiajs/jwt": "^1.4.0",
		"@elysiajs/openapi": "^1.4.11",
		"@faker-js/faker": "^10.1.0",
		"@libsql/client": "^0.15.15",
		"@nghiavuive/lunar_date_vi": "^2.0.1",
		"@sindresorhus/slugify": "^3.0.0",
		"amqplib": "^0.10.9",
		"drizzle-orm": "^0.45.0",
		"drizzle-typebox": "^0.3.3",
		"elysia": "^1.4.18",
		"meilisearch": "^0.54.0",
		"qs": "^6.14.0",
		"vnpay": "^2.4.4"
	},
	"devDependencies": {
		"dotenv": "^17.2.3",
		"@types/amqplib": "^0.10.8",
		"google-auth-library": "^10.5.0",
		"drizzle-kit": "^0.31.8",
		"@types/qs": "^6.14.0"
	},
	"module": "src/index.js"
}
